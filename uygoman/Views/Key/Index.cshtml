@*Index*@

@model IEnumerable<uygoman.Models.CrKey>






@{
    Layout = "_Layout2";
}


<br /><br />




<div class="container p-3 "   >
    <div class="row pt-4">
        <div class="col-md-6">
            <h2 class="text-primary">Encrypted</h2>
        </div>
        <div class="col-md-6">
            @*<a asp-controller="Key" asp-action="Create" class="btn btn-primary">
                New Key
            </a>*@
        </div>
    </div>


    <br /><br />
    @if (Model.Count() > 0)
    {
        <table class="table table-bordered table-striped" style="width100%">
            <thead>
                <tr>

                    <th>Keys</th>
                    <th>Product Name</th>
                    <th></th>





                </tr>
            </thead>
            <tbody>
                @foreach (var obj in Model)
                {
                <tr>
                    <td>@obj.Keys</td>
                    <td>@obj.Product_Name</td>




                    <td class="text-center">
                        <div class="btn-group" role="group">
                @*<a asp-controller="Key" asp-route-idnum="@obj.ID" asp-action="Edit" class="btn btn-primary">
                    Edit
                </a>*@
                <a asp-controller="Key" asp-route-idnum="@obj.ID" asp-action="Delete" class="btn btn-danger">
                    Delete
                </a>
            </div>

                    </td>
                </tr>
                }
            </tbody>

        </table>
    }
    else
    {
        <p>No category</p>
    }

</div>



